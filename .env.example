# Calendly API Configuration
CALENDLY_ACCESS_TOKEN=your_calendly_personal_access_token_here

# Server Configuration
NODE_ENV=production
HTTP_MODE=true
PORT=3000
HOST=0.0.0.0

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/calendly-mcp.log

# Security Configuration
ENABLE_CORS=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# N8N Integration (Optional)
N8N_WEBHOOK_URL=http://localhost:5678/webhook/calendly
N8N_WEBHOOK_SECRET=your_n8n_webhook_secret

# Features Configuration
ENABLE_WEBSOCKET=true
ENABLE_SSE=true
ENABLE_N8N=true

# Development Configuration (for local development only)
DEBUG=calendly-mcp:*
CALENDLY_TIMEOUT=30000